.box
  position:relative
  text-align:center
  margin:auto
  max-width:100%
  border-radius: 10px
  padding: 25px
  background-color: rgba(0, 0, 0, 0.6)
  
  
  ul,ol
    margin: 12px 20px
    padding:0
  li
    &::before
      left: 0.5em
  &.wait-for-gif
    opacity: 0
  &.bottom
    bottom:5%
    margin-bottom: 0
    background-color: rgba(0, 0, 0, 0.8)
  &.top
    top: 5%
    margin-top: 0
  &.left
    left: 5%
    margin-left:0
  &.right
    right: 5%
    margin-right: 0
  &.transparent
    pre
      display:inline-block

